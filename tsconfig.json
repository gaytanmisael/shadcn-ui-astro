{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@assets/*": ["assets/*"],
      "@components/*": ["components/*"],
      "@content/*": ["content/*"],
      "@data/*": ["data/*"],
      "@enums/*": ["enums/*"],
      "@icons/*": ["icons/*"],
      "@helpers/*": ["helpers/*"],
      "@libs/*": ["libs/*"],
      "@layouts/*": ["layouts/*"],
      "@models/*": ["models/*"],
      "@pages/*": ["pages/*"],
      "@stores/*": ["stores/*"],
      "@styles/*": ["styles/*"],
      "@utils/*": ["utils/*"]
    }
  }
}
